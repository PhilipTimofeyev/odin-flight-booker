<%= form_with url: "/search", method: :get do |form| %>
  <%= form.label :query, "Search for:" %>
  <%= select_tag(:departure_airport, options_for_select(@airport_options)) %>
  <%= select_tag(:arrival_airport, options_for_select(@airport_options)) %>
  <%= form.select :num_of_passengers, [1, 2, 3, 4] %>
  <%= select_tag(:start, options_for_select(@start_date)) %>
  <%= form.submit "Search" %>
<% end %>



